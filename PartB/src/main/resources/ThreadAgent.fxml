<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="335.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FrontEnd.Controller">
   <children>
      <TableView fx:id="threadTable" layoutY="4.0" prefHeight="335.0" prefWidth="670.0">
        <columns>
          <TableColumn fx:id="name" minWidth="-1.0" prefWidth="-1.0" text="Name" />
          <TableColumn fx:id="pid" minWidth="-1.0" prefWidth="-1.0" text="PID" />
            <TableColumn fx:id="state" minWidth="-1.0" prefWidth="-1.0" text="State" />
            <TableColumn fx:id="priority" minWidth="-1.0" prefWidth="-1.0" text="Priority" />
            <TableColumn fx:id="daemon" minWidth="-1.0" prefWidth="-1.0" text="Daemon" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
